<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="MultiBrowser" parallel="tests" thread-count="5">
	<parameter name="username" value="admin"></parameter>
	<parameter name="password" value="admin"></parameter>
	<test name="ChromeTest">
		<parameter name="Broswer" value="chrome">
			<classes>
				<class name="testscript.ManageNewsPageTest"></class>
			</classes>
		</parameter>
	</test>
	<test name="EdgeTest">
		<parameter name="Broswer" value="edge">
			<classes>
				<class name="testscript.ManageNewsPageTest"></class>
			</classes>
		</parameter>
	</test>
	<test name="FirefoxTest">
		<parameter name="Broswer" value="firefox">
			<classes>
				<class name="testscript.ManageNewsPageTest"></class>
			</classes>
		</parameter>
	</test>
</suite>